    "babel-jest": "^26.6.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  },
  "engines": {
    "node": "14.17.X"
  }
