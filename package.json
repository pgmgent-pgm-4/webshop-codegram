{
  "name": "course-gram",
  "version": "1.0.0",
  "description": "Webshop for Programming Courses",
  "main": "app.js",
  "scripts": {
    "babel": "./node_modules/.bin/babel server -d dist",
    "dev": "cross-env NODE_ENV=development HOSTNAME=localhost PORT=8080 nodemon --exec babel-node -e js,html,nj server/app.js --watch *",
    "lint": "cross-env NODE_ENV=lint eslint . --ext .js --fix",
    "test": "cross-env NODE_ENV=test jest --forceExit --coverage",
    "build-css": "node-sass --include-path scss server/public/_sass/main.scss server/public/css/main.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "seeder": "cross-env NODE_ENV=development HOSTNAME=localhost PORT=8080 babel-node server/db/seeders",
    "sequelize": "sequelize $*"
  },
  "jest": {
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    }
  },
  "keywords": [
    "node.js",
    "babel",
    "mvc",
    "middleware",
    "express",
    "webpack",
    "passport",
    "eslint",
    "jest"
  ],
  "author": "Team CodeGram",
  "license": "MIT",
  "dependencies": {
    "@godaddy/terminus": "^4.7.2",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "stripe": "^8.154.0",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.13.13",
    "@babel/preset-env": "^7.14.1",
    "babel-cli": "^6.26.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "faker": "^5.5.3",
    "jest": "^26.6.3",
    "node-sass": "^6.0.0",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  },
  "engines": {
    "node": "14.17.X"
  }
}
