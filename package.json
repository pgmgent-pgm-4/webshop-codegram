{
  "name": "course-gram",
  "version": "1.0.0",
  "description": "Webshop for Programming Courses",
  "main": "app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development HOSTNAME=localhost PORT=8080 nodemon --exec babel-node -e js,html,nj server/app.js --watch *",
    "lint": "cross-env NODE_ENV=lint eslint . --ext .js --fix",
    "test": "cross-env NODE_ENV=test jest --detectOpenHandles --forceExit --coverage",
    "seeder": "",
    "sass": "sass --no-source-map client/assets/_sass/main.scss client/assets/css/main.css",
    "sequelize": "sequelize $*"
  },
  "keywords": [
    "node.js",
    "babel",
    "mvc",
    "middleware",
    "express",
    "webpack",
    "passport",
    "eslint",
    "jest"
  ],
  "author": "Team CodeGram",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^9.0.2",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "nunjucks": "^3.2.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "sequelize": "^6.6.2",
    "sqlite3": "^5.0.2",
    "swagger-jsdoc": "6.1.0",
    "swagger-ui-express": "^4.1.6",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.2",
    "@babel/node": "^7.13.13",
